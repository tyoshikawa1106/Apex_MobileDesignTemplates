<apex:page showHeader="false" sidebar="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0" id="page">
    
    <apex:composition template="MobileDesignTemplete">
    
        <apex:define name="title">
            <apex:outputText value="Mobile Design Templates Apps" />
        </apex:define>
        
        <apex:define name="body">
            <apex:form id="form">
            
                <div class="app-wrapper">
                    
                    <c:MobileDesignTempletesMenu />
                    <c:MobileDesignTempletesHeader headerTitle="List View Collapsible" />
                    
                    <div class="app-content">
                        <ul class="list-view collapsable left-thumbs right-one-icons">
                            <li>
                                <div class="thumbs">
                                    <apex:image styleClass="thumb" value="{!URLFOR($Resource.Mobile_Design_Templates, 'common/images/placeholders/account-0.png')}" />
                                </div>
                                <div class="content">
                                    <h1>
                                        <apex:outputText value="Acme Inc" />
                                    </h1>
                                    <div class="list-view-icons">
                                        <span class="icon-plus">&nbsp;</span>
                                    </div>
                                </div>
                                <ul class="list-view right-one-icons">
                                    <li>
                                        <div class="content">
                                            <h2>
                                                <apex:outputText value="John Broker" />
                                            </h2>
                                            <apex:outputText value="Retail Sales" />
                                            <div class="list-view-icons">
                                                <span class="icon-right-arrow">&nbsp;</span>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="content">
                                            <h2>
                                                <apex:outputText value="Allison Amin" />
                                            </h2>
                                            <apex:outputText value="Executive Assistant" />
                                            <div class="list-view-icons">
                                                <span class="icon-right-arrow">&nbsp;</span>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <div class="thumbs">
                                    <apex:image styleClass="thumb" value="{!URLFOR($Resource.Mobile_Design_Templates, 'common/images/placeholders/account-1.png')}" />
                                </div>
                                <div class="content">
                                    <h1>
                                        <apex:outputText value="Dunder Mifflin" />
                                    </h1>
                                    <div class="list-view-icons">
                                        <span class="icon-plus">&nbsp;</span>
                                    </div>
                                </div>
                                <ul class="list-view right-one-icons">
                                    <li>
                                        <div class="content">
                                            <h2>
                                                <apex:outputText value="Regina Rodriguez" />
                                            </h2>
                                            <apex:outputText value="Operations Assistant" />
                                            <div class="list-view-icons">
                                                <span class="icon-right-arrow">&nbsp;</span>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="content">
                                            <h2>
                                                <apex:outputText value="Jason Miller" />
                                            </h2>
                                            <apex:outputText value="Regional Manager" />
                                            <div class="list-view-icons">
                                                <span class="icon-right-arrow">&nbsp;</span>
                                            </div>
                                        </div>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div><!--#app-content-->
                </div><!--#app-wrapper-->
            </apex:form>
        </apex:define>
    </apex:composition>
</apex:page>