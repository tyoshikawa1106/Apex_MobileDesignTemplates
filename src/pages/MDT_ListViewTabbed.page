                                            <apex:image styleClass="thumb thumb-3" value="{!URLFOR($Resource.Mobile_Design_Templates, 'common/images/placeholders/contact-1.png')}" />
                                        </div><!-- .thumbs -->
                                        <apex:outputLink value="#" styleClass="content">
                                            <h2>
                                                <apex:outputText value="Bruce Bryant" />
