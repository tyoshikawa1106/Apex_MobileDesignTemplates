                                        <apex:outputLink value="#" styleClass="content">
                                            <h2>
                                                <apex:outputText value="Nathan An" />
                                            </h2>
                                            <apex:outputText value="Public Relations" />
                                            <div class="list-view-icons">
                                                <span class="icon-right-arrow">&nbsp;</span>
                                            </div>
                                        </apex:outputLink>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <ul class="list-view with-swipe left-thumbs right-one-icons">
                                    <li>
                                        <div class="thumbs">
                                            <apex:outputLink value="#" styleClass="thumb thumb-1">
                                                <apex:image styleClass="thumb" value="{!URLFOR($Resource.Mobile_Design_Templates, 'common/images/icons/tile-phone.png')}" />
