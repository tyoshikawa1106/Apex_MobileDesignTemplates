<apex:page showHeader="false" sidebar="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0" id="page">
    <apex:composition template="MobileDesignTemplete">
    
        <apex:define name="title">
            <apex:outputText value="Mobile Design Templates Apps" />
        </apex:define>
        
        <apex:define name="body">
            <apex:form id="form">
                <div class="app-wrapper">
                    
                    <c:MobileDesignTempletesMenu />
                    <c:MobileDesignTempletesHeader headerTitle="Data Input Survey" />
                    
                    <div class="app-content">
                        
                        <section>
                            <div class="content">
                                <form>
                                    <div class="form-header-survey">
                                        <apex:outputText value="Question 1" />
                                        <span class="color-menu-blue">
                                            <apex:outputText value="/3" />
                                        </span>
                                    </div>
                                    <p>
                                        <apex:outputText value="Morbi id urna in diam dignissim feugiat?" />
                                    </p>
                                    <div class="form-control-group">
                                        <div class="form-control form-control-radio form-control-radio-thumbs span-50">
                                            <input type="radio" name="name" value="yes" />
                                            <label class="thumbs thumbs-up">&nbsp;</label>
                                        </div>
                                        <div class="form-control form-control-radio form-control-radio-thumbs span-50">
                                            <input type="radio" name="name" value="no" />
                                            <label class="thumbs thumbs-down">&nbsp;</label>
                                        </div>
                                    </div><!--.form-control-group-->
                                </form>
                            </div><!--.content-->
                        </section>
                        
                        <section>
                            <div class="content">
                                <form>
                                    <div class="form-header-survey">
                                        <apex:outputText value="Question 2" />
                                        <span class="color-menu-blue">
                                            <apex:outputText value="/3" />
                                        </span>
                                    </div>
                                    <p>
                                        <apex:outputText value="Nam mollis ultrices justo aenean eget mi?" />
                                    </p>
                                    <div class="form-control-group">
                                        <div class="form-control form-control-textarea">
                                            <textarea name="text"></textarea>
                                        </div>
                                    </div><!--.form-control-group-->
                                </form>
                            </div><!--.content-->
                        </section>
                        
                        <section>
                            <div class="content">
                                <form>
                                    <div class="form-header-survey">
                                        <apex:outputText value="Question 3" />
                                        <span class="color-menu-blue">
                                            <apex:outputText value="/3" />
                                        </span>
                                    </div>
                                    <p>
                                        <apex:outputText value="Nullam vitae sem at nunc pharetra ultricies?" />
                                    </p>
                                    <div class="form-control-group">
                                        <div class="form-control form-control-checkbox">
                                            <input type="checkbox" />
                                            <label for="option-1">
                                                <apex:outputText value="Option1" />
                                            </label>
                                        </div>
                                        <div class="form-control form-control-checkbox">
                                            <input type="checkbox" />
                                            <label for="option-2">
                                                <apex:outputText value="Option2" />
                                            </label>
                                        </div>
                                        <div class="form-control form-control-checkbox">
                                            <input type="checkbox" />
                                            <label for="option-3">
                                                <apex:outputText value="Option3" />
                                            </label>
                                        </div>
                                    </div><!--.form-control-group-->
                                </form>
                            </div><!--.content-->
                        </section>
                        
                        <section>
                            <div class="content">
                                <form>
                                    <div class="form-header-survey">
                                        <apex:outputText value="Thank you" />
                                    </div>
                                    <p>
                                        <apex:outputText value="Thank you for taking the time to complete this survey." />
                                    </p>
                                </form>
                            </div><!--.content-->
                        </section>
                        
                        <section class="form-navigation-buttons">
                            <div class="content">
                                <apex:outputLink value="#" styleClass="span-50 previous">
                                    <apex:outputText value="previous" />
                                </apex:outputLink>
                                <apex:outputLink value="#" styleClass="span-50 next">
                                    <apex:outputText value="next" />
                                </apex:outputLink>
                                <!-- 
                                <apex:outputLink value="#" styleClass="span-50 finish">
                                    <apex:outputText value="finish" />
                                </apex:outputLink>
                                 -->
                            </div>
                        </section><!--#form-bottom-buttons-->
                        
                    </div>
                </div>
            </apex:form>
        </apex:define>
    </apex:composition>
</apex:page>