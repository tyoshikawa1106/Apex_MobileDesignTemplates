<apex:page showHeader="false" sidebar="false" applyHtmlTag="false" applyBodyTag="false" docType="html-5.0" id="page">
    
    <apex:composition template="MobileDesignTemplete">
    
        <apex:define name="title">
            <apex:outputText value="Mobile Design Templates Apps" />
        </apex:define>
        
        <apex:define name="body">
            <apex:form id="form">
            
                <div class="app-wrapper">
                
                    <c:MobileDesignTempletesMenu />
                    <c:MobileDesignTempletesHeader headerTitle="List View Picture" />
                    
                    <div class="app-content">
                        <div class="list-view-header">
                            <apex:outputText value="A" />
                        </div>
                        <ul class="list-view with-swipe left-thumbs right-one-icons">
                            <li>
                                <div class="thumbs">
                                    <apex:outputLink value="#" styleClass="thumb thumb-1">
                                        <apex:image styleClass="thumb" value="{!URLFOR($Resource.Mobile_Design_Templates, 'common/images/icons/tile-phone.png')}" />
                                    </apex:outputLink>
                                    <apex:outputLink value="#" styleClass="thumb thumb-2">
                                        <apex:image styleClass="thumb" value="{!URLFOR($Resource.Mobile_Design_Templates, 'common/images/icons/tile-email.png')}" />
                                    </apex:outputLink>
                                    <apex:image styleClass="thumb thumb-3" value="{!URLFOR($Resource.Mobile_Design_Templates, 'common/images/placeholders/contact-2.png')}" />
                                </div>
                                <apex:outputLink value="#" styleClass="content">
                                    <h2>
                                        <apex:outputText value="Rachel Aarons" />
                                    </h2>
                                    <apex:outputText value="Project Manager" />
                                    <div class="list-view-icons">
                                        <span class="icon-right-arrow">&nbsp;</span>
                                    </div>
                                </apex:outputLink>
                            </li>
                            <li>
                                <div class="thumbs">
                                    <apex:outputLink value="#" styleClass="thumb thumb-1">
